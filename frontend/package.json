{
  "name": "docmaker-frontend",
  "private": true,
  "version": "0.4.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:headed": "playwright test --headed",
    "test:perf": "playwright test --grep @perf",
    "test:bundle": "tsc -b && vite build && node scripts/check-bundle.js",
    "qa": "npm run lint && npm run test:run && npm run test:bundle && npm run test:e2e",
    "qa:perf": "npm run test:perf",
    "test:e2e:fullstack": "playwright test --project=fullstack"
  },
  "dependencies": {
    "cytoscape": "^3.30.4",
    "cytoscape-fcose": "^2.2.0",
    "highlight.js": "^11.11.1",
    "pyloid-js": "^0.3.3",
    "react": "^19.2.4",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/cytoscape": "^3.21.8",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
